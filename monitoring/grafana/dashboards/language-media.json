{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "11.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "piechart",
      "name": "Pie chart",
      "version": ""
    },
    {
      "type": "panel",
      "id": "barchart",
      "name": "Bar chart",
      "version": ""
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    },
    {
      "type": "panel",
      "id": "gauge",
      "name": "Gauge",
      "version": ""
    }
  ],
  "id": null,
  "uid": "ebook-tools-language-media",
  "title": "ebook-tools \u2014 Language & Media",
  "description": "Generated and listened audio playtime by language and job type",
  "tags": [
    "ebook-tools",
    "language-media"
  ],
  "schemaVersion": 39,
  "version": 1,
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "refresh": "30s",
  "time": {
    "from": "now-7d",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
  },
  "timezone": "",
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 100,
      "type": "row",
      "title": "Generation Overview",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "id": 1,
      "type": "stat",
      "title": "Total Generated Hours",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum(ebook_tools_generated_playtime_seconds) / 3600",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "blue" }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Languages Covered",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 1 },
      "targets": [
        {
          "expr": "count(count by (language) (ebook_tools_generated_playtime_seconds > 0))",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "purple" }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Total Listened Hours",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "sum(ebook_tools_listened_playtime_seconds) / 3600",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Active Listeners",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 1 },
      "targets": [
        {
          "expr": "ebook_tools_playback_sessions_active",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "orange" }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      }
    },
    {
      "id": 101,
      "type": "row",
      "title": "Generated by Language",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 }
    },
    {
      "id": 5,
      "type": "piechart",
      "title": "Generated Hours by Language",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum by (language) (ebook_tools_generated_playtime_seconds) / 3600",
          "legendFormat": "{{language}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "pieType": "donut",
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "decimals": 1,
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      }
    },
    {
      "id": 6,
      "type": "barchart",
      "title": "Generated Hours by Language & Track",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "sum by (language, track_kind) (ebook_tools_generated_playtime_seconds) / 3600",
          "legendFormat": "{{language}} ({{track_kind}})",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "options": {
        "orientation": "horizontal",
        "stacking": "normal",
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "decimals": 1,
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      }
    },
    {
      "id": 102,
      "type": "row",
      "title": "Generated by Job Type",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 }
    },
    {
      "id": 7,
      "type": "piechart",
      "title": "Generated Hours by Job Type",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 19 },
      "targets": [
        {
          "expr": "sum by (job_type) (ebook_tools_generated_playtime_seconds) / 3600",
          "legendFormat": "{{job_type}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "pieType": "donut",
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "decimals": 1,
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      }
    },
    {
      "id": 8,
      "type": "gauge",
      "title": "Listen / Generate Ratio",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 19 },
      "targets": [
        {
          "expr": "sum(ebook_tools_listened_playtime_seconds) / clamp_min(sum(ebook_tools_generated_playtime_seconds), 1)",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "orientation": "auto",
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "unit": "percentunit",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 0.1, "color": "orange" },
              { "value": 0.3, "color": "yellow" },
              { "value": 0.5, "color": "green" }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      }
    },
    {
      "id": 103,
      "type": "row",
      "title": "Listening by Language",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 29 }
    },
    {
      "id": 9,
      "type": "piechart",
      "title": "Listened Hours by Language",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 30 },
      "targets": [
        {
          "expr": "sum by (language) (ebook_tools_listened_playtime_seconds) / 3600",
          "legendFormat": "{{language}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "pieType": "donut",
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "decimals": 1,
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      }
    },
    {
      "id": 10,
      "type": "barchart",
      "title": "Listen vs Generate by Language",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 30 },
      "targets": [
        {
          "expr": "sum by (language) (ebook_tools_generated_playtime_seconds) / 3600",
          "legendFormat": "Generated ({{language}})",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        },
        {
          "expr": "sum by (language) (ebook_tools_listened_playtime_seconds) / 3600",
          "legendFormat": "Listened ({{language}})",
          "refId": "B",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "options": {
        "orientation": "horizontal",
        "stacking": "none",
        "groupWidth": 0.7,
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "decimals": 1,
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      }
    },
    {
      "id": 104,
      "type": "row",
      "title": "Trends",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 40 }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Active Playback Sessions",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 41 },
      "targets": [
        {
          "expr": "ebook_tools_playback_sessions_active",
          "legendFormat": "Active sessions",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "options": {
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "pointSize": 5,
            "showPoints": "auto"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "orange" }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      }
    }
  ],
  "templating": {
    "list": []
  }
}
